#Frontend Environment Configuration
#Backend API URL - Use your local network IP for mobile development
VITE_BACKEND_URL=http://localhost:4000/

#Development Configuration
VITE_APP_NAME=SwipeCore
VITE_APP_VERSION=1.0.0

#Feature Flags
VITE_USE_MOCK_DATA=true
VITE_ENABLE_DEBUG=true
VITE_USE_LIVE_DATA=true

#Google Places API Configuration (frontend doesn't need the actual key)
VITE_GOOGLE_PLACES_ENABLED=true


#google ads...
VITE_ADMOB_APP_ID_ANDROID=
VITE_ADMOB_APP_ID_IOS=

VITE_ADMOB_NATIVE_AD_UNIT_ID_ANDROID=
VITE_ADMOB_NATIVE_AD_UNIT_ID_IOS=

VITE_ADS_NATIVE_TEST_MODE=false
VITE_ADS_ENABLED=true
VITE_ADS_TESTING=true
VITE_ADS_NATIVE_IN_DECK=true

VITE_ADS_NATIVE_INTERVAL=2
VITE_ADS_DEBUG=true

# Firebase Configuration
VITE_FIREBASE_API_KEY=your-api-key
VITE_FIREBASE_AUTH_DOMAIN=your-project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your-project-id
VITE_FIREBASE_STORAGE_BUCKET=your-project.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=123456789
VITE_FIREBASE_APP_ID=your-app-id

# Supabase Configuration
VITE_SUPABASE_URL=your-supabase-url
VITE_SUPABASE_ANON_KEY=your-supabase-anon-key

# Backend Configuration
VITE_BACKEND_URL=http://localhost:4000
VITE_USE_LIVE_DATA=true
VITE_ENABLE_LOCATION_SERVICES=true
