{
  "name": "swipecore-backend",
  "version": "1.0.0",
  "description": "Express API proxy for Google Places API",
  "main": "dist/index.js",
  "scripts": {
    "dev": "cross-env GOOGLE_API_MODE=mock ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "serve": "cross-env GOOGLE_API_MODE=live node dist/index.js",
    "test": "cross-env GOOGLE_API_MODE=mock jest --coverage",
    "test:watch": "cross-env GOOGLE_API_MODE=mock jest --watch",
    "test:coverage": "cross-env GOOGLE_API_MODE=mock jest --coverage",
    "prime": "cross-env GOOGLE_API_MODE=mock node scripts/primeFixtures.js",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix"
  },
  "keywords": [
    "express",
    "google-places",
    "proxy",
    "api"
  ],
  "author": "",
  "license": "ISC",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "axios": "^1.11.0",
    "cors": "^2.8.5",
    "cross-env": "^10.0.0",
    "dotenv": "^17.2.1",
    "dotenv-flow": "^4.1.0",
    "express": "^5.1.0",
    "express-rate-limit": "^8.0.1",
    "helmet": "^8.1.0",
    "ioredis": "^5.7.0",
    "node-cache": "^5.1.2",
    "zod": "^4.0.14"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.1.0",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "eslint": "^9.32.0",
    "jest": "^30.0.5",
    "nock": "^14.0.8",
    "prettier": "^3.6.2",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.3"
  }
}
