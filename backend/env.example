# Google Places API Configuration
GOOGLE_PLACES_API_KEY=
PLACES_BASE_URL=https://places.googleapis.com/v1

# Server Configuration
PORT=4000
NODE_ENV=development

# Rate Limiting
RATE_LIMIT_WINDOW_MS=600000
RATE_LIMIT_MAX_REQUESTS=100

# Cache Configuration
CACHE_TTL_DAYS=28    # keep â‰¤30
CACHE_TTL_NEARBY=28
CACHE_TTL_DETAILS=28
CACHE_TTL_PHOTOS=28
ENABLE_CACHE_FEATURE=true
# Dev Cache (for development mode only)
USE_DEV_CACHE=true  # Set to false to disable file-based dev caching


AWS SNS Configuration (for SMS delivery)
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_REGION=us-east-1

Database Configuration (if using database instead of in-memory storage)
DATABASE_URL=your-database-url

CORS Configuration
CORS_ORIGIN=http://localhost:3000/

Supabase Configuration
VITE_SUPABASE_URL=
VITE_SUPABASE_ANON_KEY=